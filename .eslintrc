{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "indent":"off",
        "arrow-parens":"off",
        "comma-dangle":"off",
        "require-jsdoc":"off",
        "object-curly-spacing":"off",
        "operator-linebreak":"off",
        "max-len":"off",
        "no-debugger":"warn"
    },
    "extends": ["eslint:recommended", "google"]
}